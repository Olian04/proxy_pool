{
  "name": "proxy_pool",
  "version": "0.1.0",
  "description": "An object pool specifically for proxies",
  "main": "dist/proxy-pool.js",
  "keywords": [
    "object",
    "pool",
    "proxy"
  ],
  "author": "olian04",
  "license": "MIT",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build:dev": "rm -rf ./out.dev && tsc --project ./tsconfig.build.json",
    "build:prod": "rm -rf ./out.prod && tsc --project ./tsconfig.production.json && rm -rf ./dist && rollup --config",
    "test": "npm run build:dev && vitest run ./tests/",
    "perf:time": "npm run build:dev && node -r ts-node/register --expose-gc performance/time.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.29.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "ts-node": "^10.9.2"
  }
}